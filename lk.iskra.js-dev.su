server {
    server_name lk.iskra.js-dev.su;
    root /usr/share/nginx/html;
    index index.html;
    location / {
      try_files $uri $uri/ /index.html =404;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/api.iskra.js-dev.su/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/api.iskra.js-dev.su/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = lk.iskra.js-dev.su) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name lk.iskra.js-dev.su;
    return 404; # managed by Certbot


}